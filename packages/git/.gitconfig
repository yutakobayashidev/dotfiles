[include]
path = ~/Dropbox/dotfiles/.gitconfig.local
[core]
	precomposeUnicode = true
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[ghq]
  root = ~/projects/github/yutakobayashidev
[gpg]
	program = gpg